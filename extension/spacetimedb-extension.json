{
    "name": "SpaceTimeDB",
    "fullName": "SpaceTimeDB Realtime Multiplayer",
    "version": "1.0.0",
    "author": "sixdev",
    "description": "GDevelop extension to easily integrate SpaceTimeDB for realtime multiplayer games.",
    "email": "moebius.games@gmail.com",
    "category": "Multiplayer",
    "dependencies": [],
    "eventsFunctions": [
      {
        "name": "Connect",
        "functionType": "FreeFunction",
        "description": "Connects to a SpaceTimeDB server.",
        "parameters": [
          { "type": "string", "name": "url", "description": "WebSocket URL of the server" }
        ]
      },
      {
        "name": "Subscribe",
        "functionType": "FreeFunction",
        "description": "Subscribes to a table for real-time updates.",
        "parameters": [
          { "type": "string", "name": "table", "description": "Table name" }
        ]
      },
      {
        "name": "Insert",
        "functionType": "FreeFunction",
        "description": "Inserts JSON data into a table.",
        "parameters": [
          { "type": "string", "name": "table" },
          { "type": "string", "name": "jsonData" }
        ]
      },
      {
        "name": "Update",
        "functionType": "FreeFunction",
        "description": "Updates a record in the table.",
        "parameters": [
          { "type": "string", "name": "table" },
          { "type": "string", "name": "id" },
          { "type": "string", "name": "jsonData" }
        ]
      },
      {
        "name": "Delete",
        "functionType": "FreeFunction",
        "description": "Deletes a record from a table.",
        "parameters": [
          { "type": "string", "name": "table" },
          { "type": "string", "name": "id" }
        ]
      }
    ],
    "functionsStartFile": "js/spacetimedb.js"
  }
  